    "naive-ui": "^2.30.2",
    "pinia": "^2.0.13",
    "primeicons": "^5.0.0",
    "vditor": "^3.8.15",
    "vue": "^3.2.35",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^16.11.27",
    "@vicons/antd": "^0.12.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "typescript": "~4.6.3",
